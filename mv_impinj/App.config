<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="akka" type="Akka.Configuration.Hocon.AkkaConfigurationSection, Akka" />
  </configSections>
  <akka>
    <hocon>
      <![CDATA[
        akka {
          actor{
            serializers {
              wire = "Akka.Serialization.WireSerializer, Akka.Serialization.Wire"
            }
            serialization-bindings{
              "System.Object" = wire
            }
          }
        }
      ]]>
    </hocon>
  </akka>
  <appSettings>
    <add key="ItemSenseUser" value="admin" />
    <add key="ItemSensePassword" value="admindefault" />
    <add key="ItemSenseUrl" value="http://rec30.itemsense.impinj.com" />
    <add key="MobileViewZoneMap" value="MobileViewZoneMaps"/>    <!-- the name of the zone map in itemsense-->
    <add key="AmqpNoiseTimer" value="2000"/>  <!-- the delay for noise remover, in milliseconds -->
    <add key="HttpsCertificates" value="ignore"/>
    <add key="MobileViewBase" value="https://10.200.90.52:4567"/>
    <add key="MobileViewPrefix" value ="epc:"/>
    <add key="MobileViewReports" value="/reports"/>
    <add key="MobileViewLocations" value="/locations"/>
  </appSettings>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-9.0.0.0" newVersion="9.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>